%This program plots the mean field eq.
close all
clear all

T = 1000;
b = [5 6 10];
n=1000;
figure
for j = 1:3
a_t = zeros(1,T);
a_t(1) = 1000;
for t = 1:1:3000
    t
    a_t(t) = t^2*exp(-t/n)*b/(2*n);
end
subplot(1,
plot(1:3000,1:3000, 'LineWidth', 2)
hold on
plot(1:1:3000,a_t, 'LineWidth', 2)
end